{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "0.0.0",
  "compilerOptions": {
    "outDir": "../output",
    "sourceMap": false,
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": false
  }
}